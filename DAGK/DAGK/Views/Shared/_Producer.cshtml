@model IEnumerable<WEB2Connection.Producer>

<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Producer </a>
    <div class="dropdown-menu">
        <div class="dropdown-inner">
            <ul class="list-unstyled">
                @foreach (var item in Model)
                {
                    <li><a href="@Url.Action("Index", "Producer", new {id = @item.ProducerID})">@item.ProducerName</a></li>
                }
            </ul>


        </div>
    </div>
</li>


