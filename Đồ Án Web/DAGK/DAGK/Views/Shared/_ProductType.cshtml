@model IEnumerable<WEB2Connection.ProductType>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown"> Product Type </a>  
    <div class="dropdown-menu" >
        <div class="dropdown-inner">
            <ul class="list-unstyled">
                @foreach (var item in Model)
                {
                    <li><a href="@Url.Action("Index","ProductType", new {id = @item.ProductTypeID})">@item.ProductTypeName</a></li>
                }
            </ul>

        </div>
    </div>
</li>
    